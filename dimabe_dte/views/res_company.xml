<odoo>
  <data>
    <record id="view_company_form_inherit" model="ir.ui.view">
      <field name="name">formulario de compañía</field>
      <field name="model">res.company</field>
      <field name="inherit_id" ref="base.view_company_form" />
      <field name="arch" type="xml">
        <xpath expr="//field[@name='website']" position="after">
          <field name="copies_qty"/>
          <field name="notification_remaining_caf"/>
          <field name="days_acceptation"/>
        </xpath>
      </field>
    </record>

    <record id="view_company_l10n_cl_edi_form_inherit" model="ir.ui.view">
      <field name="name">formulario de compañía</field>
      <field name="model">res.company</field>
      <field name="inherit_id" ref="l10n_cl_edi.view_company_l10n_cl_edi_form"/>
      <field name="arch" type="xml">
          <xpath expr="//page[@name='dte']" position="inside">
            <group string="Configuración para obtener Email de Contribuyentes Autorizados">
              <field name="dte_get_taxpayer_url"/>
              <field name="dte_get_taxpayer_hash"/>
              <field name="dte_get_taxpayer_customer_code"/>
            </group>
          </xpath>
      </field>
    </record>
  </data>
</odoo>