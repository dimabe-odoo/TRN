<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="stock_return_picking_inherit" model="ir.ui.view">
        <field name="name">Return Picking Inherit</field>
        <field name="model">stock.return.picking</field>
        <field name="inherit_id" ref="stock.view_stock_return_picking_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='product_return_moves']//tree//field[@name='uom_id']" position="after">
                <field name="analytic_account_id" readonly="1"/>
            </xpath>
        </field>
    </record>
</odoo>
